{
  "manifest_version": 3,
  "name": "Marca Fácil",
  "version": "1.0",
  "description": "Exctenção para aprimorar minhas experiência de estudos",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pencil.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.svg",
    "48": "images/icon48.svg",
    "128": "images/icon128.svg"
  }
}


